package dummy;

import java.util.*;
class HelloWorld {
    
        public static void kmp(String a){
        char[] A=a.toCharArray();
        int m=A.length ;
        long hA=0 , M=(long)1e9+7 , p=8191 , p1=p;
        
        for(int i=0;i<m;i++){
            hA=(hA+A[i]*p1)%M;
            p1=(p1*p)%M;
        }
        
        long val = hA;
        String str = "";
        while(val > 0){
//            int digit = (int) (val % 100); // Extract two digits at a time
//            str = (char) digit + str; // Convert the ASCII value back to a character and prepend it to the string
//            val = val / 100;
            int digit =(int) val % 100;
            if(digit > 32)str = (char) digit + str;
            else str = str + digit;
            val = val / 100;
        }
        System.out.println(str);
        System.out.println((char)(65));
      System.out.println(hA);
    }
    
    public static void main(String[] args) {
        kmp("Uday@123");
    }
}
